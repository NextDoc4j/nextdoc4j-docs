{
  "name": "nextdoc4j-docs",
  "version": "1.0.0",
  "type": "module",
  "devDependencies": {
    "@iconify-json/simple-icons": "^1.2.49",
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "unplugin-icons": "^22.2.0",
    "vitepress": "2.0.0-alpha.12"
  },
  "scripts": {
    "docs:dev": "vitepress dev",
    "docs:build": "vitepress build",
    "docs:preview": "vitepress preview",
    "team:update": "tsx update-team.ts",
    "team:force-update": "tsx update-team.ts --force",
    "team:check": "tsx -e \"import('./update-team.js').then(m => new m.default().needsUpdate().then(needs => console.log('需要更新:', needs)))\"",
    "team:status": "tsx update-team.ts --status",
    "team:cron": "node -e \"setInterval(() => import('./update-team.js').then(m => new m.default().updateTeamMembers()), 24*60*60*1000)\"",
    "prebuild": "npm run team:update",
    "type-check": "tsc --noEmit",
    "build:ts": "tsc"
  },
  "dependencies": {
    "vue": "^3.5.20"
  }
}
